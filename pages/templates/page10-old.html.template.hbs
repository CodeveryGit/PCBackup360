  <body onload="ga('send', 'event', 'form', 'load', 'step 13');">
    <div class="overlay" id="overlay">
    </div>
    <div class="overlay-message" id="overlay-message">
<img src="https://s3.amazonaws.com/image.forms.com/ajaxSpinner.gif" width="200"/>
    </div>
    <div class=wrapper>
      <div class=header>
        <div style=padding-left:40px;padding-top:7px;font-size:14px;color:#FFF>
          {{windows_pcbackup_wizard}}
        </div>
      </div>
      <div class="content">
        <div class="content-header">
<img class="header-image" src="https://s3.amazonaws.com/image.forms.com/step3-icon.png" border="0"/><span style="font-size: 28px; vertical-align: middle;font-weight: 200;">{{secure_checkout}}</span>
        </div>
        <div id="processOrder" style="padding-left: 20px;">
          <h1>
            {{please_wait_your_order_is_being_processed}}
          </h1>
        </div>
        <div id="successOrder" style="padding-left: 20px;display:none;">
          <div class="content-message">
<img class="header-image" src="https://s3.amazonaws.com/image.forms.com/1441793997_checkmark_done_complete.png" width="50" border="0"/><span style="font-size:24px;font-weight: 200;">{{congrats_your_order_was_successfully_processed}}</span>            <br/>
          </div>
          <h2>
            {{please_click_the_button_below_to_close_this_page_and_access_your_files}}
          </h2>
          <p>{{please_note_that_depending_on_the_amount_and_size_of_your_files_it_can_take_several_hours_for_all_data_to_be_accessible}}</p>
        </div>
        <div id="failOrder" style="padding-left: 20px;display:none;">
          <div class="content-message">
<img class="header-image" src="https://s3.amazonaws.com/image.forms.com/1441794035_010_x.png" width="50" border="0"/><span style="font-size:24px;font-weight: 200;">{{sorry_your_order_was_not_completed_please_try_again}}</span>            <br/>
          </div>
          <h2>
            {{please_click_the_back_button_below_to_fix_any_errors_and_try_again}}
          </h2>
        </div>
        <div class="buttons-container">
          <div id="nextButton" style="display:none;">
            <input id="next-button" type="button" class="button" value='{{close}}' tabindex="1"/>
          </div>
          <div id="backButton" style="display:none;">
            <input id="back-button" type="button" class="button" value='{{go_back}}' tabindex="1"/>
          </div>
        </div>
      </div>
      <div class="footer">
      </div>
    </div>
<script>
    function hideOverlay() {
        document.getElementById("overlay").style.display = "none";
        document.getElementById("overlay-message").style.display = "none";
        document.getElementById("processOrder").style.display = "none";
    }

    function showNext() {
        document.getElementById("nextButton").style.display = "block";
    }

    function hideNext() {
        document.getElementById("nextButton").style.display = "none";
    }

    function showBack() {
        document.getElementById("backButton").style.display = "block";
    }

    function hideBack() {
        document.getElementById("backButton").style.display = "none";
    }

    function showSuccess() {
        document.getElementById("successOrder").style.display = "block";
    }

    function showError() {
        document.getElementById("failOrder").style.display = "block";

    }

    var xhr;

    if (window.XMLHttpRequest) {
        // code for IE7+, Firefox, Chrome, Opera, Safari
        xhr = new XMLHttpRequest();
    } else {
        // code for IE6, IE5
        xhr = new ActiveXObject("Microsoft.XMLHTTP");
    }

                hideOverlay();
                showSuccess();
                showNext();

    xhr.open('POST',
            encodeURI('https://a.spdse.com/pcbackup-registration/index.php'));
    xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
    xhr.onreadystatechange = function () {
        if (xhr.readyState === 4 && xhr.status === 200) {
            var response = JSON.parse(xhr.responseText);
            if (response.status == 'success') {
                var email = response.email;
                var password = response.password;

                document.getElementById("next-button").addEventListener("click", function () {
                    external.InstallSyncClinet(email, password);
                    window.open('https://files.pcbackupwizard.net', '_blank', 'resizable=yes');
                }, false);

                hideOverlay();
                showSuccess();
                showNext();
            } else {
                hideOverlay();
                showError();
                showBack();
            }
        }
        else if (xhr.status !== 200) {
            hideOverlay();
            showError();
            showBack();
        }
    };
    xhr.send(encodeURI('guid=' + guid + '&rand=' + Math.floor((Math.random() * 100000) + 1)));
</script>
  </body>
</html>

