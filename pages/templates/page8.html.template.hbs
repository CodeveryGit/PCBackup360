  <body>
    <section id="header">
      <div class="row">
      </div>
    </section>
    <section id="info" class="page8">
      <div class="row">
        <div class="large-12 small-12 small-only-text-center header text-left heading top columns">
          <div class="large-6 medium-6 none columns">
            <h3>
              {{pc_backup_wizard_for_windows1}}
            </h3>
          </div>
          <div class="large-6 medium-6 small-12 small-only-text-center links text-right columns">
            <ul>
              <li>
<a href="page5.html"><img src="images/img_dot_blank.png" alt=""/></a>
              </li>
              <li>
<a href="page6.html"><img src="images/img_dot_blank.png" alt=""/></a>
              </li>
              <li>
<a href="page7.html"><img src="images/img_dot_blank.png" alt=""/></a>
